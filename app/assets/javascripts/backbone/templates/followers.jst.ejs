<h1 class="follower-header text-center"><%= user.get('full_name') %>'s Network</h1>
<% if (types.followers) { %>
	<h2 class="follower-subheader text-center">Members that are following <%= user.get('full_name') %>:</h2>
	<div class="timeline row followers"></div>
<% } %>
<% if (types.following) { %>
	<h2 class="follower-subheader text-center">Members that <%= user.get('full_name') %> is following:</h2>
	<div class="timeline row following"></div>
<% } %>
<h2 class="follower-network-header text-center">
	<a class="backbone" href="/members">Expand Your Network</a>
</h2>
