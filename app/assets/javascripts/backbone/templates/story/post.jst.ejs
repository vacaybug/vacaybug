<div class="panel panel-default newsfeed">
    <div class="panel-heading">
        <div class="media">
            <a href="/<%= model.get('data').user.username %>" class="backbone pull-left">
                <img src="<%= model.get('data').user.avatar.thumb %>" class="media-object avatar-thumb">
            </a>
            <div class="media-body">
                <a href="<%= model.get('data').user.username %>" class="backbone newsfeed-user"><%= model.get('data').user.full_name %></a>
                <span class="newsfeed-date"><%= $.timeago(model.get('created_at')) %></span>
            </div>
            <div class="clearfix"></div>
            <% if (model.get('data').user.id == Vacaybug.current_user.id) { %>
                <a class="js-delete-story delete" data-id="<%= model.get('data').user.id %>">&times;</a>
            <% } %>
        </div>
    </div>
    <div class="panel-body">
        <p><%= model.get('data').content %></p>
    </div>
    <% if (model.get('data').image) { %>
        <img src="<%= model.get('data').image.medium %>" class="img-responsive" width="100%">
    <% } %>
    <div class="view-all-comments">
        <a href="javascript:void(0)" class="js-like text-muted" data-toggle="modal"><i class="fa fa-heart"></i> <%= model.get('likes_count') %></a>
        <a href="javascript:void(0)" class="js-show-comments text-muted margin-left-25" data-toggle="modal"><i class="fa fa-comment"></i> <%= model.get('comments_count') %></a>
        <!-- <a href="javascript:void(0)" class="js-share text-muted margin-left-25"><i class="fa fa-refresh"></i> 10</a> -->
    </div>
    <ul class="comments">
    </ul>
</div>
